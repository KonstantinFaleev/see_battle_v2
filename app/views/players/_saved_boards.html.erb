<table class="table">
  <thead >
  <th style="text-align: center;">Saved positions</th>
  </thead>
  <tbody>
  <% @boards.each do |board| %>
      <tr>
        <td>
          <% if board.isSaved %>
              <%= link_to "#{board.id}", board %>
          <% end %>
        </td>
      </tr>
  <% end %>
  </tbody>
</table>